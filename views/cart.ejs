    <script>
        const removeForm = document.querySelectorAll('.removeBtn').forEach((form) => {
            form.addEventListener('submit', async (event) => {
                event.preventDefault();

                const productId = form.getAttribute('data-product-id');

                const response = await fetch (`/deleteCartItem/${productId}`, {
                    method: 'DELETE',
                        headers: {
                            'Content-Type': 'application/json',
                        },
                });

                const deleteData = await response.json();

                if (deleteData.success) {
                    Swal.fire({
                            icon: 'success',
                            title: 'Success',
                            text: 'Item removed'
                        }).then(() => {
                            window.location.reload();
                        });
                } else if (deleteData.isActive === false) {
                    window.location.href = '/userLogin';
                } else {
                        Swal.fire({
                            icon: 'error',
                            title: 'Error',
                            text: 'Failed to delete the item.'
                        });
                }

            })
        })
    </script>

    <script>
        document.querySelectorAll('.quantity-input').forEach(input => {
        input.addEventListener('input', function() {
            const quantity = parseInt(this.value, 10);
            const maxStock = parseInt(this.max, 10);
            const productRow = this.closest('tr');
            const productId = productRow.getAttribute('data-product-id');
            const stockMessage = productRow.querySelector('.stock-message');

            // Check if quantity exceeds maxStock
            if (quantity === maxStock) {
                console.log(`Max value ${quantity}`);
                this.value = maxStock;
                if (maxStock === 1) {
                    stockMessage.textContent = `Only ${maxStock} stock left`;
                } else if (maxStock !== 1) {
                    stockMessage.textContent = `Only ${maxStock} stocks left`;
                }
                stockMessage.style.display = 'block';
            } else {
                stockMessage.style.display = 'none';
            }

            // Send updated quantity to the server
            fetch(`/updateCart`, {
                method: 'PATCH',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    productId: productId,
                    quantity: quantity
                })
            })
            .then(response => response.json())
            .then(data => {
                if (data.success) {
                    console.log('Cart updated successfully');
                    updateProductRow(productRow, data.updatedItem);
                    updateSummary();
                } else if (data.isActive === false) {
                    window.location.href = '/userLogin';
                } else {
                    console.error('Error updating cart:', data.message);
                }
            })
            .catch(error => {
                console.error('Error:', error);
            });
        });
    });

    function updateProductRow(productRow, updatedItem) {
        const priceCol = productRow.querySelector('.price-col');
        const totalCol = productRow.querySelector('.total-col');

        if (updatedItem.discountedPrice < updatedItem.originalPrice) {
            priceCol.innerHTML = `
                <span class="original-price" style="text-decoration: line-through;">₹${updatedItem.originalPrice.toFixed(2)}</span><br>
                ₹<span class="price">${updatedItem.discountedPrice.toFixed(2)}</span>
            `;
        } else {
            priceCol.innerHTML = `₹<span class="price">${updatedItem.originalPrice.toFixed(2)}</span>`;
        }

        totalCol.querySelector('.total-price').textContent = `₹ ${updatedItem.itemTotal.toFixed(2)}`;
    }

    function updateSummary() {
        let realSubtotal = 0;
        let discountTotal = 0;
        let subtotal = 0;
        document.querySelectorAll('tr[data-product-id]').forEach(row => {
            const quantity = parseInt(row.querySelector('.quantity-input').value, 10);
            const originalPrice = parseFloat(row.querySelector('.original-price')?.textContent.replace('₹', '') || row.querySelector('.price').textContent);
            const discountedPrice = parseFloat(row.querySelector('.price').textContent);
            const totalPrice = parseFloat(row.querySelector('.total-price').textContent.replace('₹', ''));

            realSubtotal += originalPrice * quantity;
            discountTotal += (originalPrice - discountedPrice) * quantity;
            subtotal += totalPrice;
        });

        // const gst = subtotal * 0.18;
        const shipping = subtotal < 500 ? 40 : 0;
        const total = subtotal + shipping;

        document.querySelector('.summary-real-subtotal-value').textContent = `₹${realSubtotal.toFixed(2)}`;
        document.querySelector('.summary-discount-value').textContent = `-₹${discountTotal.toFixed(2)}`;
        document.querySelector('.summary-subtotal-value').textContent = `₹${subtotal.toFixed(2)}`;
        // document.querySelector('.summary-gst-value').textContent = `₹${gst.toFixed(2)}`;
        document.querySelector('.summary-shipping-value').textContent = `₹${shipping.toFixed(2)}`;
        document.querySelector('.summary-total-value').textContent = `₹${total.toFixed(2)}`;
    }
    </script>

