            <div class="page-content">
            	<div class="cart">
	                <div class="container">
	                	<div class="row">
	                		<div class="col-lg-9">
	                			<table class="table table-cart table-mobile">
                                    <thead>
                                        <tr>
                                            <th>Product</th>
                                            <th>Price</th>
                                            <th>Quantity</th>
                                            <th>Total</th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% if (cart && cart.products.length > 0) { %>
                                            <% cart.products.forEach(item => { %>
                                                <tr data-product-id="<%= item.product._id %>">
                                                    <td class="product-col">
                                                        <div class="product">
                                                            <figure class="product-media">
                                                                <a href="/prodDetails/<%= item.product._id %>">
                                                                    <img src="/assets2/img/<%= item.product.images[0] %>" alt="Product image">
                                                                </a>
                                                            </figure>
                                                            <h3 class="product-title">
                                                                <a href="/prodDetails/<%= item.product._id %>"><%= item.product.name %></a>
                                                            </h3>
                                                        </div>
                                                    </td>
                                                    <td class="price-col <% if (item.product.stock === 0) { %>text-danger<% } %>">
                                                        <% if (item.product.stock === 0) { %>
                                                            Out of stock
                                                        <% } else { %>
                                                            ₹<span class="price"><%= item.product.price %></span>
                                                        <% } %>
                                                    </td>
                                                    <td class="quantity-col">
                                                        <div class="cart-product-quantity">
                                                          <% if (item.product.stock !== 0) { %>
                                                            <input type="number" class="form-control quantity-input" value="<%= item.quantity %>" min="1" max="<%= item.product.stock < 10 ? item.product.stock : 10 %>" step="1" data-decimals="0">
                                                          <% } %>
                                                        </div>
                                                        <span class="stock-message py-3" style="color: red; display: none;"></span>
                                                    </td>
                                                    <td class="total-col"><span class="total-price <%= item.product.stock === 0 ? 'd-none' : '' %>">
                                                        <% if(item.product.stock !== 0 && item.quantity !== 0) { %>
                                                            ₹ <%= item.product.price * item.quantity %>
                                                        <% } else if (item.product.stock !== 0 && item.quantity === 0) { %>
                                                            ₹ <%= item.product.price %>
                                                        <% } else { %>
                                                            0
                                                        <% } %>
                                                    </span></td>
                                                    <td class="remove-col">
                                                        <form class="removeBtn" data-product-id="<%= item.product._id %>">
                                                            <button type="submit" class="btn-remove"><i class="icon-close"></i></button>
                                                        </form>
                                                    </td>
                                                </tr>
                                            <% }); %>
                                        <% } else { %>
                                            <tr>
                                                <td colspan="5" class="text-center">Your cart is empty.</td>
                                            </tr>
                                        <% } %>
                                    </tbody>
                                </table>

	                			<div class="cart-bottom">
                                    <!-- ////////Coupon-part///// -->
                                    <!-- ////////Coupon-part///// -->
		            			</div><!-- End .cart-bottom -->

	                		</div><!-- End .col-lg-9 -->
	                		<aside class="col-lg-3">
                                <div class="summary summary-cart">
                                    <h3 class="summary-title">Cart Total</h3>
                                    <table class="table table-summary">
                                        <tbody>
                                            <tr class="summary-subtotal">
                                                <td>Subtotal:</td>
                                                <td class="summary-real-subtotal-value">₹<%= realSubtotal.toFixed(2) %></td>
                                            </tr>
                                            <tr class="summary-discount">
                                                <td>Offer Discount:</td>
                                                <td class="summary-discount-value">-₹<%= discountTotal.toFixed(2) %></td>
                                            </tr>
                                            <tr class="summary-subtotalc d-none">
                                                <td>Subtotal (after discount):</td>
                                                <td class="summary-subtotal-value">₹<%= subtotal.toFixed(2) %></td>
                                            </tr>
                                            <tr class="summary-shipping-">
                                                <td>Shipping:</td>
                                                <td class="summary-shipping-value">₹<%= shipping.toFixed(2) %></td>
                                            </tr>
                                            <tr class="summary-total">
                                                <td>Total:</td>
                                                <td class="summary-total-value">₹<%= total.toFixed(2) %></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <a href="/checkout" class="btn btn-outline-primary-2 btn-order btn-block">PROCEED TO CHECKOUT</a>
                                </div>
                                <a href="/shop" class="btn btn-outline-dark-2 btn-block mb-3"><span>CONTINUE SHOPPING</span><i class="icon-refresh"></i></a>
                            </aside>
	                	</div><!-- End .row -->
	                </div><!-- End .container -->
