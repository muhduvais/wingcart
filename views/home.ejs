    <script>
        document.querySelectorAll('.add-to-cart').forEach(form => {
        form.addEventListener('submit', async (event) => {
            event.preventDefault();

            const productId = form.querySelector('.product-id').value;

            try {
                const response = await fetch('/addToCart', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ productId })
                });

                if (!response.ok) {
                    throw new Error(`HTTP error! Status: ${response.status}`);
                }

                const cartData = await response.json();
                console.log('Cart Data:', cartData); // Log response

                if (!cartData.user) {
                    window.location.href = '/userLogin';
                } else if (cartData.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'Success',
                        text: 'Added to cart'
                    }).then(() => {
                        window.location.reload();
                    });
                } else if (cartData.message) {
                    Swal.fire({
                        icon: 'info',
                        title: 'Info',
                        text: cartData.message
                    });
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: 'Failed to add the product.'
                    });
                }
            } catch (error) {
                console.error('Error:', error);

                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'An unexpected error occurred. Please try again later.'
                });
            }
        });
    });

