    <script>
        const btn = document.getElementById('retryBtn');

        btn.addEventListener('click', (event) => {
            event.preventDefault();
            const orderId = btn.getAttribute('data-order-id');
            retryPayment(orderId);

        })

        const retryPayment = async (orderId) => {
            try {
                const response = await fetch('/retryPayment', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({
                        orderId
                    })
                });

                const result = await response.json();

                if (result.success) {
                        console.log('Initiating Razorpay payment...');

                        const options = {
                            key: result.key,
                            amount: result.totalAmount,
                            currency: 'INR',
                            name: 'Wingcart',
                            description: 'Test Transaction',
                            image: 'https://your-logo-url.com',
                            order_id: result.razorpayOrderId,
                            handler: function (response) {
                                console.log('Payment successful. Redirecting...');
                                window.location.href = `/orderConfirmation/${result.orderId}`;
                            },
                            prefill: {
                                name: result.user.name,
                                email: result.user.email,
                                contact: result.user.phone
                            },
                            notes: {
                                address: result.address.city
                            },
                            theme: {
                                color: '#3399cc'
                            }
                        };

                        const rzp1 = new Razorpay(options);

                        // Payment failure
                        rzp1.on('payment.failed', function (response) {
                            console.error('Payment failed:', response.error);
                            Swal.fire({
                                icon: 'info',
                                title: 'Alert',
                                text: 'Payment has failed. Please try again!'
                            })
                            .then(() => {
                                window.location.href = `/orderConfirmation/${result.orderId}?payment=failed`;
                            })

                        });

                        console.log('Opening Razorpay...');
                        rzp1.open();
                } else if (result.isActive === false) {
                    window.location.href = '/userLogin';
                } else {
                    console.log('Order creation failed. Message: ', result.message);
                    Swal.fire({
                        icon: "error",
                        title: "Error",
                        text: result.message
                    })
                }
            } catch (error) {
                console.error('Error retrying payment:', error);
                Swal.fire({
                    icon: "error",
                    title: "Error",
                    text: result.message
                })
            }
        }
    </script>

    <!-- Sweet Alert -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <!-- Razorpay -->
    <script src="https://checkout.razorpay.com/v1/checkout.js"></script>
