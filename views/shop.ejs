    <script>
        document.querySelectorAll('.add-to-cart').forEach(form => {
        form.addEventListener('submit', async (event) => {
            event.preventDefault();

            const productId = form.querySelector('.product-id').value;

            try {
                const response = await fetch('/addToCart', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ productId })
                });

                if (!response.ok) {
                    throw new Error(`HTTP error! Status: ${response.status}`);
                }

                const cartData = await response.json();
                console.log('Cart Data:', cartData); // Log response

                if (!cartData.user) {
                    window.location.href = '/userLogin';
                } else if (cartData.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'Success',
                        text: 'Added to cart'
                    }).then(() => {
                        window.location.reload();
                    });
                } else if (cartData.message) {
                    Swal.fire({
                        icon: 'info',
                        title: 'Info',
                        text: cartData.message
                    });
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: 'Failed to add the product.'
                    });
                }
            } catch (error) {
                console.error('Error:', error);

                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'An unexpected error occurred. Please try again later.'
                });
            }
        });
    });
    </script>

    <script>
        document.querySelectorAll('.add-to-whishlist').forEach(form => {
        form.addEventListener('click', async (event) => {
            event.preventDefault();

            const productId = form.querySelector('.wishlist-product-id').value;

            try {
                const response = await fetch(`/addToWishlist/${productId}`, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                });

                const data = await response.json();

                if (data.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'Success',
                        text: 'Added to wishlist'
                    }).then(() => {
                        window.location.reload();
                    });
                } else if (data.isActive === false) {
                    window.location.href = '/userLogin';
                } else if (!data.success) {
                    Swal.fire({
                        icon: 'info',
                        title: 'Info',
                        text: data.message
                    });
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: 'Failed to add to wishlist.'
                    });
                }
            } catch (error) {
                console.error('Error:', error);

                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'An unexpected error occurred. Please try again later.'
                });
            }
        });
    });
    </script>

