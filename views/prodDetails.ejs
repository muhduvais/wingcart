                    <h2 class="title text-center mb-4">You May Also Like</h2><!-- End .title text-center -->

                    <div class="products mb-3">
                        <div class="row justify-content-center">
                            <% if (locals.recomProducts) { %>
                                <% recomProducts.forEach(product => { %>
                                    <div class="col-6 col-md-4 col-lg-4">
                                        <div class="product product-7 text-center">
                                            <figure class="product-media">
                                                <% if (product.stock === 0) { %>
                                                    <span class="product-label label-out">Out of Stock</span>
                                                <% } else if (isNewProduct(product.addedDate)) { %>
                                                    <span class="product-label label-new">New</span>
                                                <% } %>
                                                <a href="/prodDetails/<%= product._id %>">
                                                    <img src="/assets2/img/<%= product.images[0] %>" alt="Product image" class="product-image">
                                                </a>

                                                <div class="product-action-vertical">
                                                    <a class="btn-product-icon btn-wishlist btn-expandable"><span>add to wishlist</span></a>
                                                </div><!-- End .product-action-vertical -->

                                                <div class="product-action">
                                                    <form class="add-to-cart w-100">
                                                        <input type="hidden" class="product-id" name="productId" value="<%= product._id %>">
                                                        <% if (product.stock !== 0) { %>
                                                        <a class="btn-product btn-cart"><button type="submit" style="outline: none; border: none; background: none;">
                                                        <span>add to cart</span></button></a>
                                                        <% } %>
                                                    </form>
                                                </div><!-- End .product-action -->
                                            </figure><!-- End .product-media -->

                                            <div class="product-body">
                                                <!-- <div class="product-cat">
                                                    <a href="#">Women</a>
                                                </div>End .product-cat -->
                                                <h3 class="product-title card-text"><a href="/prodDetails/<%= product._id %>"><%= product.description %></a></h3><!-- End .product-title -->
                                                <div class="product-price">
                                                    <% if (product.stock === 0) { %>
                                                        <span class="out-price">₹<%= product.price %></span>
                                                    <% } else { %>
                                                        ₹<%= product.price %>
                                                    <% } %>
                                                </div><!-- End .product-price -->
