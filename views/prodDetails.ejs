                <div class="row">
                  <div class="col-md-6">
                    <div class="main-image-container mb-3">
                      <img id="mainImage" src="/assets2/img/<%= product.images[0] %>" alt="<%= product.name %>">
                    </div>
                    <div class="d-flex justify-content-center">
                      <% product.images.forEach((image, index) => { %>
                        <div class="thumbnail mx-2 <%= index === 0 ? 'active' : '' %>">
                          <img src="/assets2/img/<%= image %>" alt="Thumbnail" class="img-thumbnail" width="100" onclick="changeMainImage(this)">
                        </div>
                      <% }) %>
                    </div>
                  </div>
                  <div class="col-md-6">
                    <h2><%= product.name %></h2>
                    <p class="prod-details-desc"><%= product.description %></p>
                    <p class="card-text <% if (product.stock === 0) { %>text-danger<% } else { %>text-success<% } %> shop-prod-price">
                        <% if (product.stock === 0) { %>
                            Out of stock
                        <% } else { %>
                            ₹<%= product.price %>
                        <% } %>
                      </p>

                      <div class="cart-product-quantity">
                        <% if (product.stock !== 0) { %>
                            <input type="number" name="quantityInput" id="quantityInput" class="form-control" value="1" min="1" max="<%= product.stock < 10 ? product.stock : 10 %>" step="1" data-decimals="0">
                        <% } else { %>
                        <% } %>
                    </div>

                    <div class="my-4">
                      <div class="row">
                        <div class="col-6">
                            <form class="add-to-cart">
                                <input type="hidden" class="product-id" name="productId" value="<%= product._id %>">
                                <button type="submit" class="btn btn-primary btn-block w-100" <% if (product.stock === 0) { %>disabled<% } else { %><% } %> >Add to Cart</button>
                            </form>
                        </div>
                        <div class="col-6">
                          <button class="btn btn-warning btn-block" <% if (product.stock === 0) { %>disabled<% } else { %><% } %> >Add to Wishlist</button>
                        </div>
                      </div>
                    </div>
                    <!-- Additional details can be added here -->
                  </div>
                </div>
              </div>


