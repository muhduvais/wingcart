    <script>
        // Image change functionality
        const mainImage = document.getElementById('zoom-main-image');
        const galleryItems = document.querySelectorAll('.gallery-item');
    
        galleryItems.forEach(item => {
            item.addEventListener('click', function(event) {
                event.preventDefault();
    
                const newImage = this.getAttribute('data-image');
                const newZoomImage = this.getAttribute('data-zoom-image');
                
                // Update main image and zoom image
                mainImage.src = newImage;
                mainImage.setAttribute('data-zoom-image', newZoomImage);
    
                // Remove the active class from all gallery items and add it to the clicked one
                galleryItems.forEach(el => el.classList.remove('active'));
                this.classList.add('active');
    
                // Destroy existing zoom instance completely
                if ($.removeData) {
                    $.removeData(mainImage, 'elevateZoom'); // Using jQuery removeData
                } else {
                    $(mainImage).removeData('elevateZoom'); // Fallback for older jQuery
                }
                $('.zoomContainer').remove(); // Remove zoom container
    
                // Reinitialize zoom with the updated image source
                $('#zoom-main-image').elevateZoom({
                    zoomType: "lens",
                    lensShape: "round",
                    lensSize: 300,
                    containLensZoom: true,
                    cursor: "crosshair"
                });
            });
        });
    
        // Initial zoom setup
        $('#zoom-main-image').elevateZoom({
            zoomType: "lens",
            lensShape: "round",
            lensSize: 300,
            containLensZoom: true,
            cursor: "crosshair"
        });
    </script>

<script>
    document.querySelectorAll('.add-to-cart-main').forEach(form => {
    form.addEventListener('submit', async (event) => {
        event.preventDefault();

        const productId = form.querySelector('.product-id-main').value;
        const quantity = document.getElementById('qty').value;

        try {
            const response = await fetch('/addToCart', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ 
                    productId,
                    quantity
                })
            });

            if (!response.ok) {
                throw new Error(`HTTP error! Status: ${response.status}`);
            }

            const cartData = await response.json();
            console.log('Cart Data:', cartData); // Log response

            if (!cartData.user) {
                window.location.href = '/userLogin';
            } else if (cartData.success) {
                Swal.fire({
                    icon: 'success',
                    title: 'Success',
                    text: 'Added to cart'
                }).then(() => {
                    window.location.reload();
                });
            } else if (cartData.message) {
                Swal.fire({
                    icon: 'info',
                    title: 'Info',
                    text: cartData.message
                });
            } else {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'Failed to add the product.'
                });
            }
        } catch (error) {
            console.error('Error:', error);

            Swal.fire({
                icon: 'error',
                title: 'Error',
                text: 'An unexpected error occurred. Please try again later.'
            });
        }
    });
});
</script>

