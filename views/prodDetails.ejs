    <script>
        function changeMainImage(element) {
            const mainImage = document.getElementById('mainImage');
            mainImage.src = element.src;
            document.querySelectorAll('.thumbnail').forEach(thumbnail => thumbnail.classList.remove('active'));
            element.parentElement.classList.add('active');
        }

        function incrementQuantity() {
            var quantityInput = document.getElementById('quantityInput');
            var currentQuantity = parseInt(quantityInput.value);
            quantityInput.value = currentQuantity + 1;
    }

        function decrementQuantity() {
            var quantityInput = document.getElementById('quantityInput');
            var currentQuantity = parseInt(quantityInput.value);
            if (currentQuantity > 1) {
                quantityInput.value = currentQuantity - 1;
            }
    }
    </script>

    <script>
        document.querySelectorAll('.add-to-cart').forEach(form => {
        form.addEventListener('submit', async (event) => {
            event.preventDefault();

            const productId = form.querySelector('.product-id').value;
            const quantity = document.getElementById('quantityInput').value;

            try {
                const response = await fetch('/addToCart', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ productId, quantity })
                });

                if (!response.ok) {
                    throw new Error(`HTTP error! Status: ${response.status}`);
                }

                const cartData = await response.json();
                console.log('Cart Data:', cartData);

                if (cartData.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'Success',
                        text: 'Added to cart'
                    }).then(() => {
                        window.location.reload();
                    });
                } else if (cartData.message) {
                    Swal.fire({
                        icon: 'info',
                        title: 'Info',
                        text: cartData.message
                    });
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: 'Failed to add the product.'
                    });
                }
            } catch (error) {
                console.error('Error:', error);

                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'An unexpected error occurred. Please try again later.'
                });
            }
        });
    });
    </script>

