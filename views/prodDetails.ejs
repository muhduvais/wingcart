                              <form class="add-to-cart">
                                <input type="hidden" class="product-id" name="productId" value="<%= product._id %>">
                                <button type="submit" class="btn btn-primary btn-block w-100" <% if (product.stock === 0) { %>disabled<% } else { %><% } %> >Add to Cart</button>
                              </form>
                              <button class="btn btn-warning btn-block mt-2" <% if (product.stock === 0) { %>disabled<% } else { %><% } %> >Add to Wishlist</button>
                            </div>
                          </div>
                    </div>
                  <% }) %>
                <% } %>
