            <div class="page-content">
            	<div class="container">
					<table class="table table-wishlist table-mobile">
						<thead>
							<tr>
								<th>Product</th>
								<th>Price</th>
								<th>Stock Status</th>
								<th></th>
								<th></th>
							</tr>
						</thead>

						<tbody>
                            <% if (products && products.length > 0) { %>
                                <% products.forEach(product => { %>
                                    <tr>
                                        <td class="product-col">
                                            <div class="product">
                                                <figure class="product-media">
                                                    <a href="/prodDetails/<%= product._id %>">
                                                        <img src="/assets2/img/<%= product.images[0] %>" alt="Product image">
                                                    </a>
                                                </figure>
                        
                                                <h3 class="product-title">
                                                    <a href="/prodDetails/<%= product._id %>"><%= product.name %></a>
                                                </h3><!-- End .product-title -->
                                            </div><!-- End .product -->
                                        </td>
                                        <td class="price-col">
                                            <% if (product.bestOffer) { %>
                                                <!-- Display original price crossed out and offer price -->
                                                <span class="original-price">₹<%= product.price %></span>
                                                <span class="offer-price">₹<%= (product.price * (1 - (product.bestOffer.discount / 100))).toFixed(2) %></span>
                                            <% } else { %>
                                                <!-- Display the price without an offer -->
                                                ₹<%= product.price %>
                                            <% } %>
                                        </td>
                                        <td class="stock-col"><span class="in-stock <%= product.stock === 0 ? 'text-danger' : '' %>"><%= product.stock === 0 ? 'Out of stock' : 'In Stock' %></span></td>
                                        <td class="action-col">
                                            <form class="add-to-cart">
                                                <input type="hidden" class="product-id" name="productId" value="<%= product._id %>">
                                                <% if (product.stock !== 0) { %>
                                                    <a class="btn-product btn-cart" style="border: 2px solid #818181; padding: 10px 5px;">
                                                        <button type="submit" style="outline: none; border: none; background: none;">
                                                            <span>add to cart</span>
                                                        </button>
                                                    </a>
                                                <% } %>
                                            </form>
                                        </td>
                                        <td class="remove-col">
                                            <button class="btn-remove" data-product-id="<%= product._id %>"><i class="icon-close"></i></button>
                                        </td>
                                    </tr>
                                <% }) %>
                            <% } else { %>
                                <p class="py-5 text-danger"><strong>Your wishlist is empty!</strong></p>
                            <% } %>
                        </tbody>
					</table><!-- End .table table-wishlist -->
