    <script>
        document.querySelectorAll('.add-to-cart').forEach(form => {
        form.addEventListener('submit', async (event) => {
            event.preventDefault();

            const productId = form.querySelector('.product-id').value;

            try {
                const response = await fetch('/addToCart', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ productId })
                });

                if (!response.ok) {
                    throw new Error(`HTTP error! Status: ${response.status}`);
                }

                const cartData = await response.json();
                console.log('Cart Data:', cartData); // Log response

                if (!cartData.user) {
                    window.location.href = '/userLogin';
                } else if (cartData.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'Success',
                        text: 'Added to cart'
                    }).then(() => {
                        window.location.reload();
                    });
                } else if (cartData.message) {
                    Swal.fire({
                        icon: 'info',
                        title: 'Info',
                        text: cartData.message
                    });
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: 'Failed to add the product.'
                    });
                }
            } catch (error) {
                console.error('Error:', error);

                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'An unexpected error occurred. Please try again later.'
                });
            }
        });
    });
    </script>

    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const removeButtons = document.querySelectorAll('.btn-remove');

            removeButtons.forEach(button => {
                button.addEventListener('click', async (event) => {
                    const productId = event.currentTarget.getAttribute('data-product-id');

                    try {
                        const response = await fetch(`/removeFromWishlist/${productId}`, {
                            method: 'DELETE',
                            headers: {
                                'Content-Type': 'application/json'
                            }
                        });

                        const result = await response.json();

                        if (result.success) {
                            Swal.fire({
                                icon: 'success',
                                title: 'Success',
                                text: 'Item removed!'
                            }).then(() => {
                                window.location.reload();
                            });
                        } else {
                            Swal.fire({
                                icon: 'error',
                                title: 'Error',
                                text: result.message
                            });
                        }
                    } catch (error) {
                        console.error('Error removing product from wishlist:', error);
                        alert('An error occurred while removing the product.');
                    }
                });
            });
        });
    </script>

